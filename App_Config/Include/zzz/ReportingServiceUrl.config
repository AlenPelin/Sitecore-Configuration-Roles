<configuration xmlns:role="http://www.sitecore.net/xmlconfig/role/" xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
  <sitecore role:require="ContentManagement">
    <httpTransportFactory>
       <param desc="serviceUrl">http://HOST_NAME_OF_REPORTING_INSTANCE/</param>    
       <param desc="requireHttps" set:arg0="true"/>
    </httpTransportFactory>
  </sitecore>
</configuration>
